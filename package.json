{
  "name": "crane",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "libs/*"
  ],
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "prepare": "husky",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:gen": "typeorm-ts-node-commonjs migration:generate -d ormconfig",
    "migrate:create": "typeorm migration:create",
    "migrate:run": "typeorm-ts-node-commonjs migration:run -d ormconfig",
    "migrate:revert": "typeorm-ts-node-commonjs migration:revert -d ormconfig",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js src/config -r ormconfig.ts",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -r ormconfig.ts",
    "seed:runOne": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -r ormconfig.ts -s",
    "template:install": "yarn workspace @repo/email-templates install",
    "template:dev": "yarn workspace @repo/email-templates dev",
    "template:build": "yarn workspace @repo/email-templates build",
    "gen:admin": "ts-node src/commands/create-admin.command.ts",
    "command:run": "node dist/src/cli"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.821.0",
    "@aws-sdk/client-ses": "^3.808.0",
    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@azure/msal-node": "^3.6.3",
    "@bull-board/api": "^6.11.1",
    "@bull-board/express": "^6.11.1",
    "@bull-board/nestjs": "^6.11.1",
    "@illustrative-mathematics/zoom-node": "^2.1.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.1.6",
    "@nestjs/typeorm": "^11.0.0",
    "@react-email/components": "0.0.40",
    "@repo/email-templates": "workspace:*",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/passport-jwt": "^4.0.1",
    "@univerjs/core": "^0.10.9",
    "add": "^2.0.6",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "bullmq": "^5.56.1",
    "chalk": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-parser": "^3.2.0",
    "dayjs": "^1.11.13",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.5.0",
    "express-request-id": "^3.0.0",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^9.15.1",
    "google-libphonenumber": "^3.2.42",
    "googleapis": "^152.0.0",
    "helmet": "^8.1.0",
    "husky": "^9.1.7",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.18.1",
    "ms": "^2.1.3",
    "nest-commander": "^3.17.0",
    "nest-winston": "^1.10.2",
    "nestjs-i18n": "^10.5.1",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.15.6",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rrule": "^2.8.1",
    "rxjs": "^7.8.1",
    "stream-chat": "^9.10.0",
    "stripe": "^18.2.1",
    "typeorm": "^0.3.22",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^11.1.0",
    "validatorjs": "^3.22.1",
    "winston": "^3.17.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.8",
    "@types/express": "^5.0.0",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core"
    ]
  },
  "resolutions": {
    "wrap-ansi": "7.0.0"
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef"
}
